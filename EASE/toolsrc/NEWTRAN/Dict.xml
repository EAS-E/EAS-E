<entityType ID="0" isProcedure="Y">
<entityTypeName>DESTROYUSE</entityTypeName>
<Body><![CDATA[
LET REF = USEREF(AUSE)
LET DUSE = F.USAGE(REF)
WHILE DUSE NE 0
DO
	IF DUSE = AUSE
		IF LUSE NE 0
			LET S.USAGE(LUSE) = S.USAGE(DUSE)
		ELSE
			LET F.USAGE(REF) = S.USAGE(DUSE)
		ENDIF
		DESTROY DICTUSE CALLED AUSE
		LEAVE
	ENDIF
	LET LUSE = DUSE
	LET DUSE = S.USAGE(DUSE)
LOOP
RETURN
]]></Body>
