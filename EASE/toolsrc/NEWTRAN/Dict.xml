<entityType ID="0" isProcedure="Y">
<entityTypeName>FINDSPECUSE_F</entityTypeName>
<Body><![CDATA[
LET DUSE = F.USAGE(DREF)
WHILE DUSE NE 0
DO
	IF USETYPE(DUSE) EQ TYPE
		LEAVE
	ENDIF
	LET DUSE = S.USAGE(DUSE)
LOOP
RETURN WITH DUSE
]]></Body>
